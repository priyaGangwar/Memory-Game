{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
  "plugins": ["complexity", "no-duplicate-imports"],
  "rules": {
    "complexity": ["error", { "max": 10 }],
    "no-duplicate-imports": "error"
  } 
}
